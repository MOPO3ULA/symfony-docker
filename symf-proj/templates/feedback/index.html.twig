{% extends '@TwigTemplate/base.html.twig' %}

{% block body %}

    <div class="container">
        <h1>Leave your feedback here</h1>

        {{ form_start(feedback_form) }}

        {{ form_row(feedback_form.name) }}
        {{ form_row(feedback_form.email) }}
        {{ form_row(feedback_form.message) }}

        {{ form_end(feedback_form) }}
    </div>


{% endblock body %}

{% block title %}Feedback{% endblock %}